{
    "name": "webklex/laravel-pdfmerger",
    "type": "library",
    "description": "Generic PDF merger for Laravel",
    "keywords": [
        "webklex",
        "laravel",
        "pdf",
        "merger",
        "pdfmerger"
    ],
    "homepage": "https://github.com/webklex/pdfmerger",
    "license": "MIT",
    "authors": [
        {
            "name": "Malte Goldenbaum",
            "email": "github@webklex.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://www.setasign.com/downloads/"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": ">=5.0",
        "setasign/fpdf": "1.8.*",
        "setasign/fpdi_pdf-parser_eval_ioncube_php8.2": "2.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Webklex\\PDFMerger\\": "src/PDFMerger"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webklex\\PDFMerger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Webklex\\PDFMerger\\Providers\\PDFMergerServiceProvider"
            ],
            "aliases": {
                "PDFMerger": "Webklex\\PDFMerger\\Facades\\PDFMergerFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
